generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DB_CONNECTION_STRING")
}
model SupportRequest {
  id    String  @id @default(auto()) @map("_id") @db.ObjectId
  title String
  description String
  department String
  content String
}
model User {
  id    String  @id @default(auto()) @map("_id") @db.ObjectId
  username  String @unique
  password String
  fullName String
  startingPage String
}